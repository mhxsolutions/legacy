		<!--
            The <authentication> section enables configuration 
            of the security authentication mode used by 
            ASP.NET to identify an incoming user. 
        -->
		<authentication mode="Forms">
			<!-- Set the authentication timeout to an hour -->
			<forms name="SqlAuthCookie" loginUrl="~/Authentication/Login.aspx" timeout="60" />
		</authentication>
		<authorization>
			<deny users="?" />
			<allow users="*" />
		</authorization>
		<membership defaultProvider="Sql1MembershipProvider">
			<providers>
				<clear />
				<add name="Sql1MembershipProvider" connectionStringName="Sql1MembershipDatabase" applicationName="TTran" minRequiredPasswordLength="5" minRequiredNonalphanumericCharacters="0" requiresUniqueEmail="false" requiresQuestionAndAnswer="false" type="System.Web.Security.SqlMembershipProvider, System.Web, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
			</providers>
		</membership>
		<roleManager enabled="true" defaultProvider="Sql1RoleProvider">
			<providers>
				<clear />
				<add connectionStringName="Sql1MembershipDatabase" applicationName="CopsWeb" name="Sql1RoleProvider" type="System.Web.Security.SqlRoleProvider, System.Web, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
			</providers>
		</roleManager>
