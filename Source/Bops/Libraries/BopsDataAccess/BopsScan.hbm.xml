<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="BopsDataAccess.BopsScan, BopsDataAccess" table="[Scans]" lazy="false">
    <id name="ScanId" column="[POD Index]" unsaved-value="0">
      <generator class="identity" />
    </id>
    <property name="LoadRef" column="[DOC Scanned]" />
    <property name="DocTypeRef" column="[DOC Type Ref]" />
    <property name="FileName" column="[POD File]" />
    <property name="Folder" column="[POD Path]" />
    <property name="Comment" column="[Comment]" />
    <property name="Signed" column="[Signed POD]" />
    <property name="DateScanned" column="[Date Scanned]" />
    <property name="DateConfirmed" column="[Date Confirmed]" />
    <property name="ConfirmedBy" column="[Confirmed By]" />
    <property name="RejectRef" column="[Reject Ref]" />
  </class>
</hibernate-mapping>
