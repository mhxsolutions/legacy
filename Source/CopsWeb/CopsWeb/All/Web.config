<?xml version="1.0"?>

<configuration>

	<location path="ForgetPassword.aspx">
		<system.web>
			<authorization>
				<allow users="?"/>
				<allow users="*"/>
			</authorization>
		</system.web>
	</location>
	<system.web>
		<authorization>
			<deny users="?"/>
			<allow users="*" />
		</authorization>
	</system.web>

	<location path="Logout.aspx">
		<system.web>
			<authorization>
				<allow users="*"/>
				<allow users="?"/>
			</authorization>
		</system.web>
	</location>

</configuration>
