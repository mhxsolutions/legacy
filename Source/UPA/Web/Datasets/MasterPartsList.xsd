                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT [Item Templates].[Item Template ID] ItemTemplateID, 
	  Clients.[Client Name] ClientName,
	  [ATSF Product Type].[Product Type] ProductType, 
	  [ATSF Product Type].[SAP Commodity Code] SAPCommodityCode,
	  [Item Templates].[Template Part No] TemplatePartNo,
	  [Item Templates].[Template Ext Description] TemplateExtDescription, 
	  [Item Templates].[Template Group 1] TemplateGroup1, 
	  [Item Templates].[Template Group 2] TemplateGroup2, 
	  [Item Templates].[Template Group 3] TemplateGroup3, 
	  [Item Templates].[Template Group 4] TemplateGroup4, 
	  [Item Templates].[Template Group 5] TemplateGroup5, 
	  [Item Templates].[Template Alt Part No] TemplateAltPartNo, 
	  [Item Templates].[Template Weight] TemplateWeight, 
	  [Item Templates].[Template Count] TemplateCount, 
	  [Item Templates].UoM, 
	  [Item Templates].[Template Linear Ft] TemplateLinearFt, 
	  [Item Templates].[Template lb per ft] TemplateLbPerFt, 
	  [Item Templates].[QC Required] QCRequired, 
	  [Item Templates].[Template Data 1 Label] TemplateData1Label,
	  [Item Templates].[Template Data 1] TemplateData1, 
	  [Item Templates].[Template Data 2 Label] TemplateData2Label, 
	  [Item Templates].[Template Data 2] TemplateData2, 
	  [Item Templates].[Template Data 3 Label] TemplateData3Label, 
	  [Item Templates].[Template Data 3] TemplateData3,
	  [Item Templates].[Template Data float Label] TemplateDataFloatLabel, 
	  [Item Templates].[Template Data float] TemplateDataFloat
FROM ([Item Templates] 
INNER JOIN [UPA Rep Data].dbo.[ATSF Product Type] ON [Item Templates].[ATSF Product Type ref] = [ATSF Product Type].[ATSF Product ID])
INNER JOIN Clients ON [Item Templates].[Client Ref] = Clients.[Client ID]
WHERE Clients.[Client ID] = @ClientID
ORDER BY [Item Templates].[Template Part No], [Item Templates].[Template Ext Description], [Item Templates].[Template Group 1];</CommandText>
