                      <Parameter AllowDbNull="false" AutogeneratedName="ClientID" ColumnName="Client ID" DataSourceName="[UPA No Rep Data].dbo.Clients" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@ClientID" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="Client ID" SourceColumnNullMapping="false" SourceVersion="Current" />
