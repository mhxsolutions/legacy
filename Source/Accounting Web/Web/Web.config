<?xml version="1.0"?>
<!-- 
    Note: As an alternative to hand editing this file you can use the 
    web admin tool to configure settings for your application. Use
    the Website->Asp.Net Configuration option in Visual Studio.
    A full list of settings and comments can be found in 
    machine.config.comments usually located in 
    \Windows\Microsoft.Net\Framework\v2.x\Config 
-->
<configuration>
  <appSettings>
    <add key="ChartHttpHandler" value="Storage=memory;Timeout=180;Url=~/temp/;"/>
  </appSettings>
  <connectionStrings>
    <add name="DWS No Rep DataConnectionString" connectionString="Data Source=SQL4;Initial Catalog=&quot;DWS No Rep Data&quot;;Integrated Security=True" providerName="System.Data.SqlClient"/>
    <add name="DWS Rep DataConnectionString" connectionString="Data Source=SQL4;Initial Catalog=&quot;DWS Rep Data&quot;;Integrated Security=True" providerName="System.Data.SqlClient"/>
    <add name="EQ Manager ConnectionString" connectionString="Data Source=SQL4;Initial Catalog=&quot;EQ Manager&quot;;Integrated Security=True" providerName="System.Data.SqlClient"/>
    <add name="Sql1MembershipDatabase" connectionString="Initial Catalog=aspnetdb;data source=SQL4;Integrated Security=SSPI;" providerName="System.Data.SqlClient"/>
    <add name="ExecConnectionString" connectionString="Data Source=SQL4;Initial Catalog=Exec;Integrated Security=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <system.web>
    <!-- 
            Set compilation debug="true" to insert debugging 
            symbols into the compiled page. Because this 
            affects performance, set this value to true only 
            during development.
        -->
    <httpHandlers>
      <add path="ChartAxd.axd" verb="*" type="Dundas.Charting.WebControl.ChartHttpHandler" validate="false"/>
    </httpHandlers>
    <compilation debug="true" targetFramework="4.0">
      <assemblies>
        <add assembly="System.Windows.Forms, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
        <add assembly="System.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A"/>
      </assemblies>
    </compilation>
    <!--
            The <authentication> section enables configuration 
            of the security authentication mode used by 
            ASP.NET to identify an incoming user. 
        -->
    <authentication mode="Forms">
      <forms name="SqlAuthCookie" loginUrl="~/Authentication/Login.aspx" timeout="600"/>
    </authentication>
    <authorization>
      <deny users="?"/>
      <allow users="*"/>
    </authorization>
    <membership defaultProvider="Sql1MembershipProvider">
      <providers>
        <clear/>
        <add name="Sql1MembershipProvider" connectionStringName="Sql1MembershipDatabase" applicationName="AccWeb" minRequiredPasswordLength="5" minRequiredNonalphanumericCharacters="0" type="System.Web.Security.SqlMembershipProvider, System.Web, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
      </providers>
    </membership>
    <roleManager enabled="true" defaultProvider="Sql1RoleProvider">
      <providers>
        <clear/>
        <add name="Sql1RoleProvider" connectionStringName="Sql1MembershipDatabase" applicationName="AccWeb" type="System.Web.Security.SqlRoleProvider, System.Web, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
      </providers>
    </roleManager>
    <!-- <identity impersonate="true"/> -->
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
  </system.webServer>
</configuration>