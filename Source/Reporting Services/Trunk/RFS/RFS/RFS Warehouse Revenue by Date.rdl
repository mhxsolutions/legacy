    <DataSet Name="DWS_No_Rep_Data">
      <Fields>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Type">
          <DataField>Type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Count">
          <DataField>Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Revenue">
          <DataField>Revenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="FSC">
          <DataField>FSC</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DWS No Rep Data</DataSourceName>
        <CommandText>SELECT     [Warehouse Rev].[Document Date] AS Date, 'Receiving' AS Type, COUNT([Warehouse Rev].[Document Id]) AS Count, SUM([Warehouse Rev].Revenue) 
                      AS Revenue, SUM([Warehouse Rev].[Fuel Surcharge Revenue]) AS FSC
FROM         [Warehouse Rev] INNER JOIN
                      [Receivers S] ON [Warehouse Rev].[Document Id] = [Receivers S].[Receiver ID] INNER JOIN
                      [RFS Main] ON [Receivers S].[RFS ref] = [RFS Main].[RFS ID] INNER JOIN
                      tblDriverList ON [RFS Main].[Salesperson ref] = tblDriverList.EmployeeID INNER JOIN
                      [DWS Rep Data].dbo.tbldestinationList ON [RFS Main].[Shipper ref] = [DWS Rep Data].dbo.tbldestinationList.[Destination id]
WHERE     ([Warehouse Rev].[Document Type] = 1)
GROUP BY [Warehouse Rev].[Document Date]
HAVING      (@BeginDate &lt;= [Warehouse Rev].[Document Date]) AND ([Warehouse Rev].[Document Date] &lt; @EndDate)
UNION
SELECT     [Warehouse Rev_1].[Document Date] AS Date, 'Shipping' AS Type, COUNT([Warehouse Rev_1].[Document Id]) AS Count, 
                      SUM([Warehouse Rev_1].Revenue) AS Revenue, SUM([Warehouse Rev_1].[Fuel Surcharge Revenue]) AS FSC
FROM         [Warehouse Rev] AS [Warehouse Rev_1] INNER JOIN
                      [Shipping S] ON [Warehouse Rev_1].[Document Id] = [Shipping S].[Load ID] INNER JOIN
                      [RFS Main] AS [RFS Main_1] ON [Shipping S].[RFS ref] = [RFS Main_1].[RFS ID] INNER JOIN
                      tblDriverList AS tblDriverList_1 ON [RFS Main_1].[Salesperson ref] = tblDriverList_1.EmployeeID INNER JOIN
                      [DWS Rep Data].dbo.tbldestinationList AS tbldestinationList_1 ON [RFS Main_1].[Shipper ref] = tbldestinationList_1.[Destination id]
WHERE     ([Warehouse Rev_1].[Document Type] = 2)
GROUP BY [Warehouse Rev_1].[Document Date]
HAVING      (@BeginDate &lt;= [Warehouse Rev_1].[Document Date]) AND ([Warehouse Rev_1].[Document Date] &lt; @EndDate)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@BeginDate">
            <Value>=Parameters!BeginDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
